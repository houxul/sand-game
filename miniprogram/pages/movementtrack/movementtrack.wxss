/* miniprogram/pages/movementtrack/movementtrack.wxss */

page{
	height: 100%;
	overflow:hidden;
}

.action-view {
	position: absolute;
	top: 130px;
	right: 0px;
	z-index: 99;
	background-color: rgba(255, 255, 255);
	border-top-left-radius: 10px;
	border-bottom-left-radius: 10px;
	display: flex;
	flex-direction: column;
	justify-content: center;
}

.img {
	margin: 10px 10px 10px 10px;
	width: 24px;
	height: 24px;
}